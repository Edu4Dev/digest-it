{
  "name": "digest-it",
  "version": "0.2.65",
  "description": "web tools",
  "private": false,
  "workspaces": [
    "nextjs-ai-times",
    "core"
  ],
  "main": "index.js",
  "engines": {
    "node": "^20.12.2",
    "npm": "^10.5.0"
  },
  "bin": {
    "digest-it": "CLI.js"
  },
  "scripts": {
    "export": "npm run export -w core",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run dev -w core && npm run dev -w nextjs-ai-times",
    "build": "npm run build -w core && npm run build -w nextjs-ai-times && npm run export",
    "predev": "npm run sync",
    "prebuild": "npm run sync",
    "sync": "npm run sync -w core",
    "digest": "npm run digest -w core",
    "atom": "npm run atom -w core",
    "autogpt": "npm run autogpt -w core",
    "chatgpt": "npm run chatgpt -w core",
    "content": "npm run content -w core",
    "decap": "npm run decap -w core",
    "schedule": "npm run schedule -w core",
    "rss": "npm run rss -w core",
    "sass": "npm run sass -w core",
    "sitemaps": "npm run sitemaps -w core",
    "stories": "npm run stories -w core",
    "static": "npm run static -w core",
    "update": "npm run update -w core",
    "workflows": "npm run workflows -w core"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Edu4Dev/digest-it.git"
  },
  "keywords": [
    "digest"
  ],
  "author": "Milton Bolonha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Edu4Dev/digest-it/issues"
  },
  "homepage": "https://github.com/Edu4Dev/digest-it#readme",
  "dependencies": {
    "@cloudinary/url-gen": "^1.19.0",
    "@netlify/edge-functions": "^2.3.1",
    "@netlify/emails": "^1.1.0",
    "app-root-path": "^3.1.0",
    "axios": "^1.7.9",
    "cloudinary": "^2.5.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.0.0",
    "digest-it": "^0.1.47",
    "dirty-json": "^0.9.2",
    "dotenv": "^16.4.5",
    "feed": "^4.2.2",
    "foodoc": "^0.0.9",
    "fs-extra": "^11.2.0",
    "gray-matter": "^4.0.3",
    "luxon": "^3.5.0",
    "node-html-parser": "^6.1.13",
    "openai": "^4.67.0",
    "puppeteer": "^23.4.0",
    "sass": "^1.77.2",
    "sharp": "^0.33.5",
    "showdown": "^2.1.0"
  }
}
